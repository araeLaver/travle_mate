# TravelMate Web - Environment Variables Example
# Copy this file to .env and fill in your values

# API Configuration
REACT_APP_API_URL=http://localhost:8081/api
REACT_APP_WS_URL=http://localhost:8081/ws

# OAuth Configuration
REACT_APP_GOOGLE_CLIENT_ID=your-google-client-id
REACT_APP_KAKAO_APP_KEY=your-kakao-app-key
REACT_APP_NAVER_CLIENT_ID=your-naver-client-id

# Map Configuration
REACT_APP_KAKAO_MAP_API_KEY=your-kakao-map-api-key

# Feature Flags
REACT_APP_ENABLE_MOCK_DATA=false
REACT_APP_ENABLE_DEBUG_MODE=false
